{
  "name": "standing",
  "productName": "standing",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint:fix": "eslint src/**/*.{ts,tsx} --quiet --fix"
  },
  "keywords": [],
  "author": {
    "name": "Drew Teeter",
    "email": "drew@drewteeter.com"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "standing"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.tsx",
                  "name": "main_window"
                },
                {
                  "html": "./src/settings-index.html",
                  "js": "./src/renderer_settings.tsx",
                  "name": "settings_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.52",
    "@electron-forge/maker-deb": "6.0.0-beta.52",
    "@electron-forge/maker-rpm": "6.0.0-beta.52",
    "@electron-forge/maker-squirrel": "6.0.0-beta.52",
    "@electron-forge/maker-zip": "6.0.0-beta.52",
    "@electron-forge/plugin-webpack": "6.0.0-beta.52",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/pouchdb-adapter-idb": "^6.1.3",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.34.0",
    "css-loader": "^3.0.0",
    "electron": "9.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "node-loader": "^0.6.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "date-fns": "^2.15.0",
    "electron-squirrel-startup": "^1.0.0",
    "keytar": "^6.0.1",
    "menubar": "^9.0.1",
    "particle-api-js": "^9.0.2",
    "pouchdb-adapter-idb": "^7.2.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.4.1",
    "rxdb": "^9.5.0",
    "rxjs": "^6.6.2",
    "tailwindcss": "^1.6.2",
    "tailwindcss-textshadow": "^2.1.3",
    "uuid": "^8.3.0"
  }
}
